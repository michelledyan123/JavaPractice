package Assignment1;

import java.util.*;
import java.lang.*;


public class pet {
	private String name;
	private int age;
	private String location;
	private String type;
	
	//initialize
	public pet() {
		name = "";
		age = 0;
		location = "";
		type = "";
	}
	
	//set data
	public void setData(String name){
		this.name = name; 
	}

}
